    },
    "Essays": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::essay.essay",
      "inversedBy": "Sources"
    },
    "Networks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::network.network",
      "inversedBy": "Sources"
    },
    "People": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::person.person",
      "inversedBy": "Sources"
    }
